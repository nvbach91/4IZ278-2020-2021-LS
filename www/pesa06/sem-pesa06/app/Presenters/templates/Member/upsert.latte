                <div class="form-group row">
                    <div class="col-md-4">
                        {label yearOfBirth}
                    </div>
                    <div class="col-md-6">
                        <input n:name="yearOfBirth" class="form-control" style="background-color: #ffcccb">
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-md-4">
                        {label email}
                    </div>
                    <div class="col-md-6">
                        <input type="email" n:name="email" class="form-control">
                    </div>
                </div>
                <div class="text-right">
                    <button type="submit" n:name="submit" class="btn btn-outline-info">Odeslat</button>
