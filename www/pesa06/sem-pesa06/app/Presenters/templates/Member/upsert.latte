{block content}
<div class="container align-content-center">
    <div class="card">
        <div class="card-header">
            Editace ƒçlena
        </div>
        <div class="card-body">
            <form n:name="upsertMemberForm">
                <div class="form-group row">
                    <div class="col-md-4">
                        {label firstName}
                    </div>
                    <div class="col-md-6">
                        <input n:name="firstName" class="form-control" style="background-color: #ffcccb">
                    </div>
                </div>
                <div class="form-group row">
                    <div class="col-md-4">
