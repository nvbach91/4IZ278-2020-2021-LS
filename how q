[32m+[m[32m<?php[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32minterface DatabaseOperations[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    public function fetchAll();[m[41m[m
[32m+[m[32m    public function create($item);[m[41m[m
[32m+[m[32m    public function fetchBy($id, $value);[m[41m[m
[32m+[m[32m    public function deleteBy($id, $value);[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mabstract class Database implements DatabaseOperations[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    protected $pdo;[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public function __construct()[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        $this->pdo = new PDO([m[41m[m
[32m+[m[32m            'mysql:host=localhost;dbname=coworking;charset=utf8mb4',[m[41m [m
[32m+[m[32m            'root',[m[41m [m
[32m+[m[32m            ''[m[41m[m
[32m+[m[32m        );[m[41m[m
[32m+[m[32m        $this->pdo->setAttribute(PDO::ATTR_ERRMODE, PDO::ERRMODE_EXCEPTION);[m[41m[m
[32m+[m[32m        $this->pdo->setAttribute(PDO::ATTR_DEFAULT_FETCH_MODE, PDO::FETCH_ASSOC);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public function fetchAll() {[m[41m[m
[32m+[m[32m        $sql = 'SELECT * FROM ' . $this->tableName;[m[41m[m
[32m+[m[32m        $statement = $this->pdo->prepare($sql);[m[41m[m
[32m+[m[32m        $statement->execute();[m[41m[m
[32m+[m[32m        return $statement->fetchAll();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public function fetchBy($id, $value)[m[41m[m
[32m+[m[32m    {[m[41m[m
[32m+[m[32m        $sql = 'SELECT * FROM ' . $this->tableName .  ' WHERE ' . $id . ' = :value';[m[41m[m
[32m+[m[32m        $statement = $this->pdo->prepare($sql);[m[41m[m
[32m+[m[32m        $statement->execute(['value' => $value]);[m[41m[m
[32m+[m[32m        return $statement->fetchAll();[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    public function deleteBy($id, $value) {[m[41m[m
[32m+[m[32m        $sql = 'DELETE FROM ' . $this->tableName . ' WHERE ' . $id . ' = :value';[m[41m[m
[32m+[m[32m        $statement = $this->pdo->prepare($sql);[m[41m[m
[32m+[m[32m        $statement->execute(['value' => $value]);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[41m [m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m?>[m
