[32m+[m[32m<?php[m[41m[m
[32m+[m[41m[m
[32m+[m[32msession_start();[m[41m[m
[32m+[m[32mrequire __DIR__ . '/db.php';[m[41m[m
[32m+[m[41m[m
[32m+[m[32mif (!empty($_POST)) {[m[41m[m
[32m+[m[32m    $email = $_POST['email'];[m[41m[m
[32m+[m[32m    $password = $_POST['password'];[m[41m[m
[32m+[m[32m    $confirmPassword = $_POST['password_confirm'];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    if (!filter_var($email, FILTER_VALIDATE_EMAIL)) {[m[41m[m
[32m+[m[32m        $errors['email'] = 'Please enter valid email address';[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    if ($password != $confirmPassword) {[m[41m[m
[32m+[m[32m        $errors['password'] = 'Your passwords do not match.';[m[41m[m
[32m+[m[32m        $errors['confirmPassword'] = 'Your passwords do not match.';[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m    else if (strlen($password) < 5) {[m[41m[m
[32m+[m[32m        $errors['password'] = 'Your password must be at least 5 characters long.';[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    if (empty($errors)) {[m[41m[m
[32m+[m[32m        $hashPassword = password_hash($password, PASSWORD_DEFAULT);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        $register = $pdo->prepare("[m[41m[m
[32m+[m[32m            INSERT INTO users (email, password)[m[41m[m
[32m+[m[32m            VALUES (:email, :password);[m[41m[m
[32m+[m[32m            ");[m[41m[m
[32m+[m[32m        $register->execute([[m[41m[m
[32m+[m[32m            "email" => $email,[m[41m[m
[32m+[m[32m            "password" => $hashPassword[m[41m[m
[32m+[m[32m        ]);[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        $login = $pdo->prepare("[m[41m[m
[32m+[m[32m            SELECT * FROM users WHERE email = :email"[m[41m[m
[32m+[m[32m         );[m[41m[m
[32m+[m[32m            $login->execute([[m[41m[m
[32m+[m[32m            'email' => $email[m[41m[m
[32m+[m[32m        ]);[m[41m[m
[32m+[m[32m        $user = $login->fetchAll()[0];[m[41m[m
[32m+[m[41m[m
[32m+[m[32m        if (password_verify($password, $user['password'])) {[m[41m[m
[32m+[m[32m            $_SESSION['email'] = $user['email'];[m[41m[m
[32m+[m[32m            $_SESSION['user_id'] = $user['id'];[m[41m[m
[32m+[m[32m            $_SESSION['user_privilege'] = $user['privilege'];[m[41m[m
[32m+[m[32m            header('Location: index.php');        }[m[41m[m
[32m+[m[32m    }}[m[41m[m
[32m+[m[41m[m
[32m+[m[32m?>[m[41m[m
[32m+[m[41m[m
