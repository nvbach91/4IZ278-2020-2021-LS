[32m+[m[32m<?php[m[41m[m
[32m+[m[32m$errorMessages = [];[m[41m[m
[32m+[m[41m[m
[32m+[m[32mfunction sendMail($name, $email, $message)[m[41m[m
[32m+[m[32m{[m[41m[m
[32m+[m[32m    $recepient = 'kadm08@vse.cz';[m[41m[m
[32m+[m[32m    $subject = $name;[m[41m[m
[32m+[m[32m    $headers = [[m[41m[m
[32m+[m[32m        'MIME-Version: 1.0',[m[41m[m
[32m+[m[32m        'Content-type: text/html, charset=utf-8',[m[41m[m
[32m+[m[32m        'From: ' . $email,[m[41m[m
[32m+[m[32m        'Reply-To: ' . $email,[m[41m[m
[32m+[m[32m        'X-Mailer: PHP/8.0',[m[41m[m
[32m+[m[32m    ];[m[41m[m
[32m+[m[32m    return mail($recepient, $subject, $message, implode("\r\n", $headers));[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[41m[m
[32m+[m[32mif (!empty($_POST)) {[m[41m[m
[32m+[m[32m    $name = htmlspecialchars(trim($_POST['name']));[m[41m[m
[32m+[m[32m    $email = htmlspecialchars(trim($_POST['email']));[m[41m[m
[32m+[m[32m    $message = htmlspecialchars(trim($_POST['message']));[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    if (!$name || !preg_match('/^[a-zA-Z ]*$/', $name)) {[m[41m[m
[32m+[m[32m        array_push($errorMessages, 'Please enter your name');[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    if (!filter_var($email, FILTER_VALIDATE_EMAIL)) {[m[41m[m
[32m+[m[32m        array_push($errorMessages, 'Please enter valid email address');[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[41m[m
[32m+[m[32m    if (empty($errorMessages)) {[m[41m[m
[32m+[m[32m        sendMail($name, $email, $message);[m[41m[m
[32m+[m[32m    }[m[41m[m
[32m+[m[32m}[m[41m[m
[32m+[m[32m?>[m[41m[m
[32m+[m[41m[m
